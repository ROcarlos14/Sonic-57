{
    "rewrites": [
        {
            "source": "/api/tracks/:id",
            "destination": "/api/tracks/[id].js"
        },
        {
            "source": "/api/tracks",
            "destination": "/api/tracks.js"
        },
        {
            "source": "/api/seed",
            "destination": "/api/seed.js"
        },
        {
            "source": "/api/health",
            "destination": "/api/health.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}